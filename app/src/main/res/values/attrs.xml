<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="KdaBar">
        <attr name="killCount" format="integer" />
        <attr name="deathCount" format="integer" />
        <attr name="assistCount" format="integer" />

        <attr name="killBarColor" format="color" />
        <attr name="deathBarColor" format="color" />
        <attr name="assistBarColor" format="color" />
    </declare-styleable>

    <declare-styleable name="Minimap">
        <attr name="direTowers" format="integer" />
        <attr name="radiantTowers" format="integer" />

        <attr name="direBarracks" format="integer" />
        <attr name="radiantBarracks" format="integer" />

        <attr name="radiantWin" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ExpandableView">
        <attr name="topLayout" format="reference" />
        <attr name="bottomLayout" format="reference" />
    </declare-styleable>
</resources>